/* components/common/ye-search/ye-search.wxss */
.search-container{
    width: 100%;
    border-radius: 40rpx;
    overflow: hidden;
    display: flex;
    background: white;
    align-items: center;
    box-sizing: border-box;
    padding-left: 10rpx;
    border: 1rpx solid black;
}
.search{
    flex: 1;
    padding: 10rpx 10rpx;
    line-height: 1.4;
    font-size: var(--normal-font-size);
    height: calc(var(--normal-font-size)*1.4);
}